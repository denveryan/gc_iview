{
  "name": "gc_iview",
  "version": "1.3.1",
  "description": "green cloud ui library macked by vue and iview",
  "main": "dist/index.umd.min.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:dev": "vue-cli-service build --dest ./dist/dev --target lib --name index ./src/index.js --report",
    "build:docs": "vue-cli-service build --dest ./docs/assets/dist --target lib --name index ./src/index.js",
    "build:prod": "vue-cli-service build --dest ./dist --target lib --name index ./src/index.js --no-clean",
    "docs": "docsify serve docs",
    "lint": "vue-cli-service lint"
  },
  "repository": "git+https://github.com/huqiliang/gc_iview.git",
  "keywords": [
    "green",
    "cloud",
    "ui",
    "iview"
  ],
  "author": "huqiliang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huqiliang/gc_iview/issues"
  },
  "homepage": "https://github.com/huqiliang/gc_iview#readme",
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.1",
    "core-js": "^2.6.5",
    "iview": "^3.1.3",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "vue": "^2.5.17"
  },
  "peerDependencies": {
    "axios": "^0.18.1",
    "iview": "^3.1.3",
    "lodash": "^4.17.11",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
